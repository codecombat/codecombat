    "firepad": "~0.1.2",
    "marked": "~0.3.0",
    "moment": "~2.5.0",
    "aether": "~0.5.6",
    "underscore.string": "~2.3.3",
    "firebase": "~1.0.2",
    "d3": "~3.4.4",
    "jsondiffpatch": "0.1.8",
    "nanoscroller": "~0.8.0",
    "treema": "https://github.com/codecombat/treema.git#master",
