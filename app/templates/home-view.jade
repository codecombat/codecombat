extends /templates/base-flat.jade

block content
  .container-fluid
    .row#jumbotron-container-fluid
      .col-lg-6
        h1.text-white(data-i18n="new_home.slogan")
        div.button-section
          if me.isAnonymous()
            p
              button.btn.btn-lg.btn-primary.teacher-btn(data-event-action="Homepage Click Teacher Button #1 CTA", data-i18n="new_home.im_an_educator", style="text-transform: none")
            p
              button.btn.btn-lg.btn-primary.student-btn(data-event-action="Homepage Click Student Button CTA", data-i18n="new_home.im_a_student", style="text-transform: none")
            p
              a.btn.btn-lg.btn-primary-alt.play-btn(href="/play", data-i18n="common.play", data-event-action="Homepage Play Now CTA", style="text-transform: none")
          else
            p
              if me.isTeacher()
                a.btn.btn-lg.btn-primary.my-classes-btn(href="/teachers/classes", data-event-action="Homepage Click My Classes CTA", data-i18n="new_home.go_to_my_classes")
              else if me.isStudent()
                a.btn.btn-lg.btn-primary.my-courses-btn(href="/students", data-event-action="Homepage Click My Courses CTA", data-i18n="new_home.go_to_courses")
              else
                a.btn.btn-lg.btn-primary.continue-playing-btn(href="/play", data-event-action="Homepage Campaign Continue Playing CTA", data-i18n="courses.continue_playing")

    .row#game-based.vertical-align
        .col-md-5
          .game-based-blurb
            h3.text-navy(data-i18n="new_home.game_based_blurb")
            if me.isAnonymous()
              br
              p
                a.btn.btn-lg.btn-primary.play-btn(href="/play", data-event-action="Homepage Try The Game CTA", data-i18n="new_home.try_the_game")
        .col-md-7
          .game-based-image
            img(src="/images/pages/home/type_real_code_logo.gif"
              alt="CodeCombat character Anya follows typed code block to reach chest of gems")

    if !me.showingStaticPagesWhileLoading()
      .row.row-dark#classroom-in-box-row
        .col-lg-12
          .container
            .row
              .col-lg-7.col-md-12
                .video-iframe
                  if me.showChinaVideo()
                    video(controls width="640" height="350" poster="/images/pages/home/video_thumb.png" preload="metadata")
                      source(src="https://assets.koudashijie.com/CoCo%E7%AE%80%E4%BB%8B.mp4" type="video/mp4")
                      a(href="https://assets.koudashijie.com/CoCo%E7%AE%80%E4%BB%8B.mp4")
                        img(src="/images/pages/home/video_thumb.png" style="width: 640px; height:350px")
                  else
                    iframe.vimeo-player(src="https://player.vimeo.com/video/229304323", frameborder="0", webkitallowfullscreen, mozallowfullscreen, allowfullscreen, width="640" height="350")
              .col-lg-5.col-md-12
                h2(data-i18n="new_home.classroom_in_box_title")
                img(src="/images/pages/home/pink_rectangle.png"
                alt="")
                ul
                  li
                    span.text-p(data-i18n="new_home.classroom_in_box_blurb1")
                  li
                    span.text-p(data-i18n="new_home.classroom_in_box_blurb2")
                  li
                    span.text-p(data-i18n="new_home.classroom_in_box_blurb3")
            if me.isAnonymous()
              .row.signup_btn
                button.btn.btn-lg.btn-primary.signup-button.signup-home-btn(data-event-action="Homepage Sign Up #2 CTA", data-i18n="signup.sign_up")

    .row#creativity-rigor
      .col-lg-12
        h1.text-center.text-navy(data-i18n="new_home.creativity_rigor_title")
        .container
          .row
            .col-lg-4
              img(src="/images/pages/home/creativity_rigor1.png", width="363", height="204"
                  alt="Girl plays CodeCombat on a computer while smiling")
              .tile-shadow
                h3.text-teal(data-i18n="new_home.creativity_rigor_subtitle1")
                p.text-navy(data-i18n="new_home.creativity_rigor_blurb1")
            .col-lg-4
              img(src="/images/pages/home/creativity_rigor2.png", width="363", height="204"
                  alt="Two girls share a computer with CodeCombat playing between them")
              .tile-shadow
                h3.text-teal(data-i18n="new_home.creativity_rigor_subtitle2")
                p.text-navy(data-i18n="new_home.creativity_rigor_blurb2")
            .col-lg-4
              img(src="/images/pages/home/creativity_rigor3.png", width="363", height="204"
                  alt-="Smiling professional teacher")
              .tile-shadow
                h3.text-teal(data-i18n="new_home.creativity_rigor_subtitle3")
                p.text-navy(data-i18n="new_home.creativity_rigor_blurb3")
        if me.isAnonymous()
          .text-center
            button.btn.btn-lg.btn-primary.request-quote(data-event-action="Homepage Request Demo", data-i18n="new_home.request_quote")

    .row.row-dark.text-center#built-for-teachers
