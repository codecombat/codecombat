extends /templates/base-flat

block content
  .container-fluid
    .row#jumbotron-container-fluid
      .col-lg-5
      .col-lg-7
        h1.text-gold(data-i18n="new_home.slogan")
        div.button-section
          if me.isAnonymous()
            p
              button.btn.btn-lg.btn-primary.signup-button(data-i18n="new_home.get_started")
            p
              a.btn.btn-lg.btn-primary-alt.play-btn(href="/play", data-i18n="new_home.try_the_game")
          else
            p
              if me.isTeacher()
                a.btn.btn-lg.btn-primary(href="/teachers/classes", data-i18n="new_home.go_to_my_classes")
              else if me.isStudent()
                a.btn.btn-lg.btn-primary(href="/students", data-i18n="new_home.go_to_my_classes")
              else
                a.btn.btn-lg.btn-primary(href="/play", data-i18n="courses.continue_playing")

    .row#game-based
        .col-md-5
          .game-based-blurb
            h3.text-navy(data-i18n="new_home.game_based_blurb")
            if me.isAnonymous()
              br
              p
                a.btn.btn-lg.btn-primary.play-btn(href="/play", data-event-action="Homepage Play Now CTA", data-i18n="new_home.try_the_game")
        .col-md-7
          .game-based-image
            img(src="/images/pages/home/type_real_code_classroom.png"
              alt="CodeCombat character Anya follows typed code block to reach chest of gems")

    .row.row-dark#classroom-in-box-row
      .col-lg-12
        .container
          .row
            .col-lg-6.vid-link
              a.open-video-btn.video-thumbnail(data-toggle="modal", data-target="#screenshot-lightbox", data-index='4')
                img(src="/images/pages/home/video_thumb.png"
                    alt="Teacher assists student with their program video thumbnail")
            .col-lg-6
              h3 At CodeCombat, We Strive To:
              img(src="/images/pages/home/pink_rectangle.png"
               alt="")
              ul.text-p
                li(data-i18n="new_home.classroom_in_box_blurb1")
                li(data-i18n="new_home.classroom_in_box_blurb2")
                li(data-i18n="new_home.classroom_in_box_blurb3")
              if me.isAnonymous()
                br
                button.btn.btn-lg.btn-primary.signup-button(data-event-action="Homepage Click Student Button CTA", data-i18n="signup.sign_up")

    .row#creativity-rigor
