.left-wing

.right-wing

#loading-details.loading-container

  .level-loading-goals.secret.rtl-allowed
    .goals-title(data-i18n=view.goalHeaderTransationKey) Goals
    ul.list-unstyled(dir="auto")

  .errors

  .intro-doc.hidden
    .nano
      .nano-content
        .intro-doc-content(dir="auto")

  .progress-or-start-container
    button.start-level-button.btn.btn-lg.btn-success.btn-illustrated.header-font.needsclick(data-i18n=view.buttonTranslationKey) Start Level

    .load-progress
      .progress
        .progress-background
        .progress-bar-container
          .progress-bar.progress-bar-success
        .rim

  .could-not-load
    span(data-i18n="loading_error.could_not_load")

  .subscription-required
    span(data-i18n="subscribe.subscription_required_to_play")
    button.start-subscription-button.btn.btn-lg.btn-warning(data-i18n="subscribe.subscribe_title")

  .course-membership-required
    span(data-i18n="courses.course_membership_required_to_play")
    a.btn.btn-lg.btn-warning(data-i18n="courses.back_courses", href="/students")
    
  .license-required
    span(data-i18n="courses.license_required_to_play")
    a.btn.btn-lg.btn-warning(data-i18n="courses.back_courses", href="/students")

